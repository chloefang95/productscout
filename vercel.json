{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "analyze_app/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "reach_app/main.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze/(.*)",
      "dest": "analyze_app/main.py"
    },
    {
      "src": "/api/reach/(.*)",
      "dest": "reach_app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/dist/index.html"
    }
  ]
} 